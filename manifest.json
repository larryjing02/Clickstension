{
    "manifest_version": 2,
    "name": "Clickstension",
    "description": "Helps click stuff on webpages!",
    "version": "1.0.0",
    "icons": { "128": "clickIcon128.png" },
    "browser_action": {
      "default_icon": "clickIcon.png",
      "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
  }